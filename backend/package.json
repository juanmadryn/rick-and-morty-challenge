{
  "name": "rick-and-morty-challenge-backend",
  "version": "0.1.0",
  "description": "Backend para el proyecto Rick And Morty Challenge de Chipax",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js test/features --require test/tests.setup.js --require test/features/**/*.ts",
    "watch-server": "nodemon --watch src -e ts,tsx --exec ts-node src/server.ts",
    "build-ts": "tsc",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/server.js",
    "build": "npm run lint && npm run build-ts",
    "start": "npm run serve",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanmadryn/rick-and-morty-challenge.git"
  },
  "keywords": [
    "rick",
    "morty",
    "challenge",
    "angular",
    "node",
    "chipax"
  ],
  "author": "Juan Manuel Cortez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juanmadryn/rick-and-morty-challenge/issues"
  },
  "homepage": "https://github.com/juanmadryn/rick-and-morty-challenge#readme",
  "dependencies": {
    "@cucumber/cucumber": "^7.0.0",
    "@types/json-diff": "^0.5.1",
    "@types/koa-send": "^4.1.2",
    "@types/koa-static": "^4.0.1",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "json-diff": "^0.5.4",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "koa-swagger-decorator": "^1.7.4",
    "sync-request": "^6.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/cucumber": "^7.0.0",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.14.33",
    "chai": "^4.3.4",
    "cucumber": "^7.0.0-rc.0",
    "cucumber-pretty": "^6.0.0",
    "cucumber-tsflow": "^4.0.0-rc.1",
    "eslint": "^7.21.0",
    "husky": "^5.1.3",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  }
}
